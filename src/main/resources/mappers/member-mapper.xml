<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC
"-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="memberMapper">
	
	<select id="loginMember" resultType="Member" parameterType="Member">
		<![CDATA[
			SELECT * FROM MEMBER 
			WHERE MEMBER_ID = #{member_id} 
			AND MEMBER_PWD = #{member_pwd}
		]]>
	</select>

	<!-- <select id="selectAll" resultMap="">
		<![CDATA[
		select * from member
		]]>
	</select> -->
	
	<insert id="insertMember" parameterType="Member" 
		useGeneratedKeys="true">
		<![CDATA[
		insert into member values (#{member_id}, #{member_pwd}, 
		#{member_name}, #{gender}, #{age}, #{email}, #{phone}, 
		#{address}, #{hobby}, default)
		]]>
	</insert>




</mapper>
